ALTER TABLE Delete_Review ADD CONSTRAINT Delete_Review_Student_id
    FOREIGN KEY (Student_id_First_name)
    REFERENCES Student_id (First_name)  
    NOT DEFERRABLE 
    INITIALLY IMMEDIATE
;

