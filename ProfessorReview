--Table: Administrator
CREATE TABLE Administrator (
    fName char  NOT NULL,
    lName char  NOT NULL,
    adminEmail char  NOT NULL,
    CONSTRAINT Administrator_pk PRIMARY KEY (fName)
);

-- Table: Feedback
CREATE TABLE Feedback (
    rating char  NOT NULL,
    reviewID char  NOT NULL,
    input char  NOT NULL,
    CONSTRAINT Feedback_pk PRIMARY KEY (rating)
);

-- Table: Professor
CREATE TABLE Professor (
    fName char NOT NULL,
    lName char NOT NULL,
    profEmail char NOT NULL,
    CONSTRAINT Professor_pk PRIMARY KEY (profEmail)
);

-- Table: Student
CREATE TABLE Student (
    fName char NOT NULL,
    lName char NOT NULL,
    studentEmail char NOT NULL,
    CONSTRAINT Student_pk PRIMARY KEY (studentEmail)
);

-- End of file.

