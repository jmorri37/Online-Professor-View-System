ALTER TABLE Professor_Search ADD CONSTRAINT Professor_Search_Student_id
    FOREIGN KEY (Student_id_First_name)
    REFERENCES Student_id (First_name)  
    NOT DEFERRABLE 
    INITIALLY IMMEDIATE
;

